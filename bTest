package com.nielsen.model;

import java.util.List;

import com.fasterxml.jackson.annotation.JsonProperty;
import com.fasterxml.jackson.databind.annotation.JsonDeserialize;

public class Or {
	private List<Or> or;

	public List<Or> getOr() {
		return or;
	}

	public void setOr(List<Or> or) {
		this.or = or;
	}

	@JsonProperty("eq")
	String[] eq;
	@JsonProperty("and")
	List<And> and;
	@JsonProperty("le")
	String[] le;
	@JsonProperty("ge")
	String[] ge;

	@JsonProperty("gt")
	String[] gt;

	@JsonProperty("lt")
	String[] lt;

	@JsonProperty("ne")
	String[] ne;
	
	@JsonProperty("geq")
    private List<String> geq;
	
	@JsonProperty("Col_NM")
	private String colNM;
	
	@JsonProperty("File")
	private String file;
	
	public List<String> getGeq() {
		return geq;
	}

	public void setGeq(List<String> geq) {
		this.geq = geq;
	}

	public String getColNM() {
		return colNM;
	}

	public void setColNM(String colNM) {
		this.colNM = colNM;
	}

	public String getFile() {
		return file;
	}

	public void setFile(String file) {
		this.file = file;
	}

	public Filter getFilter() {
		return filter;
	}

	public void setFilter(Filter filter) {
		this.filter = filter;
	}

	@JsonDeserialize(using = FilterDeserializer.class)
	@JsonProperty("Filters")
	private Filter filter;
	

	@JsonProperty("le")
	public String[] getLe() {
		return le;
	}

	@JsonProperty("le")
	public void setLe(String[] le) {
		this.le = le;
	}

	@JsonProperty("ge")
	public String[] getGe() {
		return ge;
	}

	@JsonProperty("ge")
	public void setGe(String[] ge) {
		this.ge = ge;
	}

	@JsonProperty("gt")
	public String[] getGt() {
		return gt;
	}

	@JsonProperty("gt")
	public void setGt(String[] gt) {
		this.gt = gt;
	}

	@JsonProperty("lt")
	public String[] getLt() {
		return lt;
	}

	@JsonProperty("lt")
	public void setLt(String[] lt) {
		this.lt = lt;
	}

	@JsonProperty("ne")
	public String[] getNe() {
		return ne;
	}

	@JsonProperty("ne")
	public void setNe(String[] ne) {
		this.ne = ne;
	}

	@JsonProperty("and")
	public List<And> getAnd() {
		return and;
	}

	@JsonProperty("and")
	public void setAnd(List<And> and) {
		this.and = and;
	}

	@JsonProperty("eq")
	public String[] getEq() {
		return eq;
	}

	@JsonProperty("eq")
	public void setEq(String[] eq) {
		this.eq = eq;
	}

}
